{
  "name": "eslint-plugin-ariel",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.13.1",
  "description": "A collection of ESLint rules.",
  "author": "Ariel Salgado <ariel.salgado.acevedo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ariel-salgado/eslint-plugin-ariel#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ariel-salgado/eslint-plugin-ariel.git"
  },
  "keywords": [
    "eslint-plugin"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "check": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@ariel-salgado/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "@typescript/native-preview": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "resolutions": {
    "eslint-plugin-ariel": "workspace:*"
  }
}
